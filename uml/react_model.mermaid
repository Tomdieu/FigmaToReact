classDiagram
    class ReactApplication {
        +String name
        +String version
        +addComponent(Component)
        +configureRouter()
    }

    class JSXElement {
        <<abstract>>
        +String key
        +Map~String,PropTypes~ propTypes
        +Array~JSXElement~ children
        +render()
        +validateProps()
    }

    class Component {
        +String name
        +String type
        +Props defaultProps
        +Props propTypes
        +render()
    }

    class ClassComponent {
        +Map~String,any~ state
        +Map~String,Function~ lifecycleMethods
        +setState(newState)
        +componentDidMount()
        +componentDidUpdate()
        +componentWillUnmount()
    }

    class FunctionalComponent {
        +useHooks()
        +returnJSX()
    }

    class HTMLElement {
        +String tagName
        +Boolean isSelfClosing
        +String namespace
        +validateElement()
    }

    class Props {
        +Map~String,PropDefinition~ definitions
        +Boolean childrenAllowed
        +validateProps()
        +getDefaultProps()
    }

    class PropDefinition {
        +String name
        +String type
        +any defaultValue
        +Boolean isRequired
        +Function validator
    }

    class State {
        <<abstract>>
        +String name
        +any value
        +update()
        +subscribe()
        +unsubscribe()
    }

    class LocalState {
        +Boolean isPrivate
        +initializeState()
        +setState()
        +getState()
    }

    class GlobalState {
        +String scope
        +Map subscribers
        +dispatch()
        +select()
    }

    class Context {
        +any defaultValue
        +Provider provider
        +Consumer consumer
        +createContext()
    }

    class Hook {
        <<abstract>>
        +String name
        +apply()
        +cleanup()
    }

    class StateHook {
        +any initialState
        +Function setState
    }

    class EffectHook {
        +Function effect
        +Array dependencies
    }

    class CustomHook {
        +String name
        +Array~Hook~ usedHooks
    }

    class HTMLAttribute {
        <<abstract>>
        +String name
        +String value
        +Boolean isRequired
        +validateValue()
    }

    class GlobalAttribute {
        +String id
        +String className
        +String style
        +Boolean hidden
    }

    class EventAttribute {
        +String eventName
        +Function handler
        +Boolean preventDefault
    }

    class Router {
        +String basename
        +String mode
        +configureRoutes()
    }

    class Route {
        +String path
        +Boolean exact
        +Component component
    }

    class Children {
        +Array~JSXElement~ elements
        +map(function)
        +count()
    }

    JSXElement <|-- Component : extends
    JSXElement <|-- HTMLElement : extends
    Component <|-- ClassComponent : extends
    Component <|-- FunctionalComponent : extends
    
    Component --> "1" Props : defines
    Props --> "*" PropDefinition : contains
    
    Component --> "*" State : manages
    State <|-- LocalState : extends
    State <|-- GlobalState : extends
    State <|-- Context : extends
    
    Component --> "*" Hook : uses
    Hook <|-- StateHook : extends
    Hook <|-- EffectHook : extends
    Hook <|-- CustomHook : extends
    
    HTMLElement --> "*" HTMLAttribute : has
    HTMLAttribute <|-- GlobalAttribute : extends
    HTMLAttribute <|-- EventAttribute : extends
    
    ReactApplication --> "1" Router : has
    Router --> "*" Route : manages
    ReactApplication --> "*" Component : contains
    ReactApplication --> "*" Context : provides
    Component --> "1" Children : has

    note for JSXElement "Base class for all
    renderable elements"

    note for Component "User-defined
    React components"

    note for HTMLElement "Built-in HTML
    elements"

    note for State "Base class for all
    state types"

    note for Hook "Base class for React hooks"

    note for HTMLAttribute "Base class for
    HTML attributes"