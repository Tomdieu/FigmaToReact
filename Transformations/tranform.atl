-- Helper pour les styles de navigation
helper context FigmaMM!FRAME def: getNavStyles(): String =
    let baseStyles: String = self.getFrameStyles() in
    let navProps: Sequence(String) = Sequence{} in
    
    -- Navigation spcifique
    let withNavDisplay: Sequence(String) =
        navProps->append('"display": "flex"')
               ->append('"alignItems": "center"')
    in
    
    let withNavGap: Sequence(String) =
        if not self.itemSpacing.oclIsUndefined() and self.itemSpacing > 0 then
            withNavDisplay->append('"gap": "' + self.itemSpacing.toString() + 'px"')
        else
            withNavDisplay->append('"gap": "16px"')
        endif
    in
    
    let withNavPadding: Sequence(String) =
        withNavGap->append('"padding": "12px 16px"')
    in
    
    -- Join navigation styles
    let navSpecificStyles: String =
        withNavPadding->iterate(prop; acc: String = '' |
            if acc = '' then
                prop
            else
                acc + ', ' + prop
            endif
        )
    in
    
    -- Combine with base styles
    if baseStyles <> '' and navSpecificStyles <> '' then
        baseStyles + ', ' + navSpecificStyles
    else if baseStyles <> '' then
        baseStyles
    else
        navSpecificStyles
    endif endif;

