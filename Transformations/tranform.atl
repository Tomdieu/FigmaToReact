helper context FigmaMM!FRAME def: isSelect(): Boolean =
    let lowercaseName: String = self.name.toString().toLower() in
    (
        lowercaseName.indexOf('dropdown') <> -1 or
        lowercaseName.indexOf('select') <> -1 or
        lowercaseName.indexOf('combo') <> -1 or
        lowercaseName.indexOf('picker') <> -1
