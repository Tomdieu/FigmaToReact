    let withProgressAppearance: Sequence(String) =
        progressProps->append('"-webkit-appearance": "none"')
                     ->append('"appearance": "none"')
    in
    
    let withProgressDimensions: Sequence(String) =
        if not self.absoluteBoundingBox.oclIsUndefined() then
            withProgressAppearance->append('"width": "' + self.absoluteBoundingBox.width.toString() + 'px"')
                                  ->append('"height": "' + self.absoluteBoundingBox.height.toString() + 'px"')
        else
            withProgressAppearance->append('"width": "200px"')
                                  ->append('"height": "20px"')
        endif
    in
    
    let withProgressColors: Sequence(String) =
        if not self.backgroundColor.oclIsUndefined() then
            let r: Real = self.backgroundColor.r * 255 in
            let g: Real = self.backgroundColor.g * 255 in
            let b: Real = self.backgroundColor.b * 255 in
            withProgressDimensions->append('"backgroundColor": "rgb(' + r.floor().toString() + ', ' + g.floor().toString() + ', ' + b.floor().toString() + ')"')
        else
            withProgressDimensions->append('"backgroundColor": "#e0e0e0"')
        endif
    in
    
    let withProgressBorder: Sequence(String) =
        withProgressColors->append('"borderRadius": "10px"')
                          ->append('"border": "none"')
    in
    
    -- Join progress styles
    let progressSpecificStyles: String =
        withProgressBorder->iterate(prop; acc: String = '' |
            if acc = '' then
                prop
            else
                acc + ', ' + prop
            endif
        )
    in
    
    -- Combine with base styles
    if baseStyles <> '' and progressSpecificStyles <> '' then
        baseStyles + ', ' + progressSpecificStyles
    else if baseStyles <> '' then
        baseStyles
    else
        progressSpecificStyles
    endif endif;

